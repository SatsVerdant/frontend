# Copy to .env.local in this directory and adjust values
# Client-exposed vars must be prefixed with NEXT_PUBLIC_

# App
NEXT_PUBLIC_APP_NAME=SatsVerdant
NEXT_PUBLIC_ENV=development

# Analytics (optional)
NEXT_PUBLIC_VERCEL_ANALYTICS=0

# API endpoints (adjust when backend is available)
NEXT_PUBLIC_API_BASE=http://localhost:8000

# Feature flags
NEXT_PUBLIC_ENABLE_MARKETPLACE=true
NEXT_PUBLIC_ENABLE_GOVERNANCE=true
